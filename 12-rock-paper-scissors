public Integer rockPaperScissors(String player1, String player2) {
    String a = player1;
    string b = player2;
    if(a == b) return 0;
    if(a == 'rock' && b == 'paper') return 2;
    if(b == 'rock' && a == 'paper') return 1;
    if(a == 'rock' && b == 'scissors') return 1;
    if(b == 'rock' && a == 'scissors') return 2;
    if(a == 'paper' && b == 'scissors') return 1;
    if(b == 'scissors' && a == 'paper') return 2;
}